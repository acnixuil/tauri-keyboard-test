{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-keyboard-test",
  "version": "0.1.0",
  "identifier": "com.tauri-keyboard-test",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
          {
            "title": "tauri-keyboard-test",
            "width": 1100,       
            "height": 450,       
            "minWidth": 1100,    
            "minHeight": 450,
            "resizable": true,
            "fullscreen": false
          }
        ],
        "security": {
          "csp": null
        }
      },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
